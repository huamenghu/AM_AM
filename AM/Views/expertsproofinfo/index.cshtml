@{
    Layout = "~/Views/Shared/_LayoutGrid.cshtml";
}
<div id="Dlg-Edit" title="窗口" style="width: 670px; height: 400px;" buttons="#buttons">
    <div style="padding: 10px 10px 40px 40px;">
        <form method="post">
            <table>
                <tr>
                    <td class="right">公司名称：
                    </td>
                    <td>
                        <input name="proofcompanyname" class="easyui-validatebox editInput" required='true' />
                    </td>
                </tr>
                <tr>
                    <td class="right">公司地址：
                    </td>
                    <td>
                        <input name="proofcompanyaddr" class="easyui-validatebox editInput" />
                    </td>
                </tr>
                <tr>
                    <td class="right">工程名称：
                    </td>
                    <td>
                        <input name="proofnenginename" class="easyui-validatebox editInput" />
                    </td>
                </tr>
                <tr>
                    <td class="right">工程地址：
                    </td>
                    <td>
                        <input name="proofnengineaddr" class="easyui-validatebox editInput" />
                    </td>
                </tr>
                <tr>
                    <td class="right">方案设计单位：
                    </td>
                    <td>
                        <input name="proofdesigncompany" class="easyui-validatebox editInput" />
                    </td>
                </tr>
                <tr>
                    <td class="right">产品名称：
                    </td>
                    <td>
                        <input name="proofgoodsname" class="easyui-validatebox editInput" />
                    </td>
                </tr>
                <tr>
                    <td class="right">产品型号：
                    </td>
                    <td>
                        <input name="proofgoodstype" class="easyui-validatebox editInput" />
                    </td>
                </tr>
                <tr>
                    <td class="right">方案类型：
                    </td>
                    <td>
                        <input name="prooftype" class="easyui-validatebox editInput" />
                    </td>
                </tr>
                <tr>
                    <td class="right">计划论证时间：
                    </td>
                    <td>
                        <input type="text" name="proofplandate" editable="false" class="easyui-datebox" />
                    </td>
                </tr>
                <tr>
                    <td class="right">申请时间：
                    </td>
                    <td>
                        <input type="text" name="proofapplicationdate" editable="false" class="easyui-datebox" />
                    </td>
                </tr>
                <tr>
                    <td class="right">联系人：
                    </td>
                    <td>
                        <input name="prooflinkname" class="easyui-validatebox editInput" />
                    </td>
                </tr>
                <tr>
                    <td class="right">联系电话：
                    </td>
                    <td>
                        <input name="prooflinktel" class="easyui-validatebox editInput" />
                    </td>
                </tr>
                <tr>
                    <td class="right">邮箱：
                    </td>
                    <td>
                        <input name="proofemail" class="easyui-validatebox editInput" />
                    </td>
                </tr>
                <tr>
                    <td class="right">状态：
                    </td>
                    <td>
                        <input name="state" id="state" class="easyui-combobox editInput" data-options="textField:'Value',valueField:'Key',url:'/Tools/GetState'" />
                    </td>
                </tr>
                <tr>
                    <td class="right">是否支付：
                    </td>
                    <td>
                        <input name="ispay" type="radio" value="true" />已支付
                        <input name="ispay" type="radio" value="false" />未支付
                    </td>
                </tr>
            </table>
        </form>
    </div>
</div>
<div id="tb" style="padding: 3px 0px 3px 10px; height: auto">
    <div style="vertical-align: middle">
        公司名称：
                        <input id="s_proofemail" class="easyui-validatebox searchInput" />
        联系人：
                        <input id="s_prooflinkname" class="easyui-validatebox searchInput" />
        联系电话：
                        <input id="s_prooflinktel" class="easyui-validatebox searchInput" />
        方案类型：
                        <input id="s_prooftype" class="easyui-validatebox searchInput" />
        处理状态：
                    <input id="s_state" class="easyui-combobox searchInput" data-options="textField:'Value',valueField:'Key',url:'/Tools/GetState2'" />
        <a href="javascript:void(0)" class="easyui-linkbutton" iconcls="icon-search" onclick="findData()">查询</a>
    </div>
</div>
@section scripts{
    <script>
        var tableName = "expertsproofinfo";
        var sortName = "GUID";
        var idField = "GUID";
        var sortOrder = "asc";

        var columns = [[
    {
        field: 'prooflinkname', title: '联系人', width: 100, sortable: true
    }, {
        field: 'prooflinktel', title: '联系电话', width: 100, sortable: true
    }, {
        field: 'proofcompanyname', title: '公司名称', width: 100, sortable: true
    }, {
        field: 'proofapplicationdate', title: '申请时间', width: 100, sortable: true
    }, {
        field: 'ispay', title: '是否支付', width: 100, sortable: true
    }, {
        field: 'state', title: '处理状态', width: 100, sortable: true
    }]];
        function toExcel() {

        }

        function returnQueryParams() {
            var obj = {
                proofemail: $("#s_proofemail").val(),
                prooflinkname: $("#s_prooflinkname").val(),
                prooflinktel: $("#s_prooflinktel").val(),
                prooftype: $("#s_prooftype").val(),
                state: $("#s_state").combobox("getValue"),
            };
            return obj;
        }
    </script>
}

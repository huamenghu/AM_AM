        var columns = [[
                        { field: 'sbxh', title: '设备型号', width: 100, sortable: true },
                        { field: 'azdw', title: '安装单位', width: 100, sortable: true },
                        { field: 'sccj', title: '生产厂家', width: 100, sortable: true },
                        { field: 'tsjbh', title: '提升机编号', width: 100, sortable: true },
                        { field: 'aqscxkzbh', title: '安全生产许可证编号', width: 100, sortable: true },

        ]];
        function toExcel() {
            var arr = getSelectedArr(datagrid, idField);
            if (arr.length > 0) {
                $.messager.confirm('提示信息', '您确认要导出选中的所有记录吗?', function (data) {
                    if (data) {
                        window.open("/" + tableName + "/Export/" + arr.join(','));
                    }
                });
            } else {
                msgShow('请先选择要导出的记录。');
            }
        }

        function returnQueryParams() {
            var obj = {
                trainname: $("#s_trainname").val(),
                //traintel: $("#s_traintel").val(),
                //trainID: $("#s_trainID").val(),
                //traintypeid: $("#s_traintypeid").combobox("getValue"),
                //state: $("#s_state").combobox("getValue"),
            };
            return obj;
        }
    </script>
}





